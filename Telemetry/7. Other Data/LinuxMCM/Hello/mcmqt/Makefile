####### Compile

.obj/main.o: main.cpp .ui/mcmCom.h \
		decode.h \
		exec.h \
		cfile.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/main.o main.cpp

.obj/cfile.o: cfile.cpp cfile.h \
		decode.h \
		exec.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/cfile.o cfile.cpp

.obj/decode.o: decode.cpp cfile.h \
		decode.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/decode.o decode.cpp

.obj/exec.o: exec.cpp cfile.h \
		exec.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/exec.o exec.cpp

.ui/mcmCom.h: mcmCom.ui decode.h \
		exec.h \
		cfile.h
	$(UIC) mcmCom.ui -o .ui/mcmCom.h

.ui/mcmCom.cpp: .ui/mcmCom.h mcmCom.ui mcmCom.ui.h decode.h \
		exec.h \
		cfile.h
	$(UIC) mcmCom.ui -i mcmCom.h -o .ui/mcmCom.cpp

.obj/mcmCom.o: .ui/mcmCom.cpp mcmCom.ui.h \
		.ui/mcmCom.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/mcmCom.o .ui/mcmCom.cpp

.obj/moc_mcmCom.o: .moc/moc_mcmCom.cpp  .ui/mcmCom.h decode.h \
		exec.h \
		cfile.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_mcmCom.o .moc/moc_mcmCom.cpp

.moc/moc_mcmCom.cpp: $(MOC) .ui/mcmCom.h
	$(MOC) .ui/mcmCom.h -o .moc/moc_mcmCom.cpp

.obj/qmake_image_collection.o: .ui/qmake_image_collection.cpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/qmake_image_collection.o .ui/qmake_image_collection.cpp

.ui/qmake_image_collection.cpp: images/editcopy \
		images/editcut \
		images/editpaste \
		images/filenew \
		images/fileopen \
		images/filesave \
		images/print \
		images/redo \
		images/searchfind \
		images/undo
	$(UIC)  -embed mcmcom images/editcopy images/editcut images/editpaste images/filenew images/fileopen images/filesave images/print images/redo images/searchfind images/undo -o .ui/qmake_image_collection.cpp

